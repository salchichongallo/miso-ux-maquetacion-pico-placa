{
  "name": "miso-ux-maquetacion-pico-placa",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "mobile:dev": "vite --config vite.config.mobile.ts",
    "mobile:build": "tsc && vite --config vite.config.mobile.ts --emptyOutDir build",
    "mobile:preview": "vite --config vite.config.mobile.ts preview",
    "web:dev": "vite --config vite.config.web.ts",
    "web:build": "tsc && vite --config vite.config.web.ts --emptyOutDir build",
    "web:preview": "vite --config vite.config.web.ts preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier -w .",
    "sync:android": "cap sync android",
    "open:android": "cap open android",
    "android": "cap run android",
    "build:android": "cd android && ./gradlew assembleDebug && cd .."
  },
  "dependencies": {
    "@capacitor/android": "7.0.1",
    "@capacitor/core": "7.0.1",
    "clsx": "2.1.1",
    "react": "^18.2.0",
    "react-aria": "3.37.0",
    "react-aria-components": "1.6.0",
    "react-dom": "^18.2.0",
    "react-icons": "5.5.0",
    "react-stately": "3.35.0"
  },
  "devDependencies": {
    "@capacitor/cli": "7.0.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "3.5.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
