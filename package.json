{
  "name": "miso-ux-maquetacion-pico-placa",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "mobile": "vite --config vite.config.mobile.ts",
    "build:mobile": "tsc && vite --config vite.config.mobile.ts --emptyOutDir build",
    "mobile:preview": "vite --config vite.config.mobile.ts preview",
    "web": "vite --config vite.config.web.ts",
    "build:web": "tsc && vite --config vite.config.web.ts --emptyOutDir build",
    "web:preview": "vite --config vite.config.web.ts preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier -w .",
    "sync": "cap sync android",
    "build": "npm-run-all build:*",
    "open:android": "cap open android",
    "android": "cap run android",
    "android:build": "cd android && ./gradlew assembleDebug && cd .."
  },
  "dependencies": {
    "@capacitor/android": "7.0.1",
    "@capacitor/core": "7.0.1",
    "@capacitor/ios": "7.0.1",
    "@internationalized/date": "3.7.0",
    "@tailwindcss/vite": "4.0.12",
    "clsx": "2.1.1",
    "formik": "2.4.6",
    "react": "18.3.1",
    "react-aria": "3.37.0",
    "react-aria-components": "1.6.0",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "react-router-dom": "7.3.0",
    "react-stately": "3.35.0",
    "tailwindcss": "4.0.12"
  },
  "devDependencies": {
    "@capacitor/cli": "7.0.1",
    "@react-aria/optimize-locales-plugin": "1.1.4",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "8.57.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.19",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vite": "5.4.14"
  }
}
